  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=development babel-node node_modules/.bin/webpack --watch --config config/webpack/webpack.api.babel.ts",
    "dev:clean": "rimraf '.build/development*/{*,.*}' 'node_modules/.cache'",
    "dev:watch": "nodemon --config config/nodemon/dev.json",
    "prod:build": "NODE_ENV=production BABEL_ENV=production babel-node node_modules/.bin/webpack --config config/webpack/webpack.api.babel.ts",
    "prod:clean": "rimraf '.build/production/{*,.*}' 'node_modules/.cache'",
    "prod:run": "NODE_ENV=production node .build/production/node/api.js",
    "prod:watch": "nodemon --config config/nodemon/prod.json",
    "lint": "yarn eslint; yarn tsc; yarn tslint",
    "lint:fix": "yarn eslint:fix; yarn tsc; yarn tslint:fix",
    "lint:watch": "nodemon --config config/nodemon/lint.json",
    "lint:fix:watch": "nodemon --config config/nodemon/lint.fix.json",
    "eslint": "eslint --report-unused-disable-directives --format codeframe '**/*.{js,jsx,ts,tsx,json}'",
    "eslint:fix": "yarn eslint --fix",
    "eslint:watch": "nodemon --config config/nodemon/eslint.json",
    "eslint:fix:watch": "nodemon --config config/nodemon/eslint.fix.json",
    "tsc": "tsc --project tsconfig.json --noEmit",
    "tsc:watch": "yarn tsc --watch",
    "tslint": "tslint --project tsconfig.json --config tslint.json --format codeFrame",
    "tslint:watch": "nodemon --config config/nodemon/tslint.json",
    "tslint:fix": "yarn tslint --fix",
    "tslint:fix:watch": "nodemon --config config/nodemon/tslint.fix.json",
    "prettier": "prettier --check '**/*.{js,jsx,ts,tsx,json,html,css,less,scss}'",
    "prettier:fix": "yarn prettier --write",
    "test": "jest --config=config/jest/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config=config/jest/jest.config.js --runInBand --watch",
    "test:coverage": "NO_ESLINT=1 yarn test --coverage",
    "packages:upgrade": "ncu --upgrade",
    "packages:upgrade:newest": "ncu --upgrade --newest"
  },
